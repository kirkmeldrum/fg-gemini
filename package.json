{
  "name": "foodgenie",
  "version": "0.1.0",
  "private": true,
  "description": "FoodGenie â€” AI-powered kitchen companion platform",
  "scripts": {
    "dev": "pnpm --filter @foodgenie/api dev & pnpm --filter @foodgenie/web dev",
    "dev:api": "pnpm --filter @foodgenie/api dev",
    "dev:web": "pnpm --filter @foodgenie/web dev",
    "build": "pnpm -r build",
    "build:api": "pnpm --filter @foodgenie/api build",
    "build:web": "pnpm --filter @foodgenie/web build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "db:migrate": "pnpm --filter @foodgenie/api db:migrate",
    "db:seed": "pnpm --filter @foodgenie/api db:seed",
    "db:reset": "pnpm --filter @foodgenie/api db:reset",
    "clean": "pnpm -r clean && rimraf node_modules"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}